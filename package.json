{
  "name": "puppeteer-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test": "jest --config jest.config.js ./test/components",
    "test:e2e": "jest --config jest.e2e.config.js --runInBand ./test/e2e",
    "test:e2e-on-ci": "jest --config jest.e2e.config.js --runInBand ./test/e2e --reporters='default' --reporters='<rootDir>/test/e2e/utils/image-reporter.ts'",
    "testServer": "nuxt build && nuxt start --port 3000"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.11.0",
    "nuxt": "^2.13.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@types/jest": "^26.0.9",
    "@types/jest-image-snapshot": "^3.1.0",
    "@types/puppeteer": "^3.0.1",
    "@vue/test-utils": "^1.0.3",
    "aws-sdk": "^2.728.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.0.1",
    "jest-image-snapshot": "^4.1.0",
    "jest-puppeteer": "^4.4.0",
    "jest-puppeteer-preset": "^4.4.0",
    "prettier": "^2.0.5",
    "puppeteer": "^5.2.1",
    "serverless": "^1.78.1",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7",
    "vue-jest": "^3.0.4"
  }
}
